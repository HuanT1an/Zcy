#include<iostream>
using namespace std;
void main() {
	double sum = 0, s ;
	printf("n e\n");
	printf("- -----------\n");
	for (int i = 0;i < 10;i++) {
		if (i == 0)
			s = 1;
		else 
			s = s*i;
		sum += 1 / s;
		if (i <= 2)
			printf("%d %g\n", i, sum);
		else
			printf("%d %.9lf\n", i, sum);
		
	}
}
